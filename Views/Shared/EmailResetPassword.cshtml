@model DreamMessenger.Models.AccountModels.ResetPasswordViewModel

<html>
<body>
<table width="100%" cellspacing="0" cellpadding="0" border="0" style="max-width: 600px;-webkit-text-size-adjust: 100%;-ms-text-size-adjust: 100%;">
