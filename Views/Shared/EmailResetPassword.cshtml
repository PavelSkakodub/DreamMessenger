                          <tr>
                            <td class="o_pbtn o_text-md o_br-max o_bg-white" align="center" data-bgcolor="White" data-size="Text Medium" data-min="16" data-max="22" style="padding: 14px 32px;vertical-align: top;background-color: #ffffff;border-radius: 56px;font-size: 18px;line-height: 27px;">
                              <a class="o_text-headline o_text-dark" href="@Model.CallbackUrl" data-color="Text Dark" style="font-family:sans-serif;word-break: break-word;font-weight: bold;color: #212932;text-decoration: none;outline: none;display: block;">
                                  <span data-color="Text Dark" style="color: #212932;">Reset My Password</span>
